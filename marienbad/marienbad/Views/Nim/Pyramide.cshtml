@for (int i = 0; i < Model.Items.Count; i++)
{
    <div style="margin-bottom: 10px">
        <div class="btn-group" role="group" aria-label="..." >
            @for (int j = 0; j < Model.Items[i]; j++)
            {

                <button class="btn btn-default" data-row="@i" onClick="toggleMe(this)"><i class="fa fa-exclamation fa-3x"></i></button>

            }

        </div>
    </div>
    <script>var myModel = @Html.Raw(Json.Encode(Model));</script>
}
